    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "bluebird": "^3.7.2",
    "chart.js": "^3.5.1",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
